{
  "name": "hono-app-boilerplate",
  "version": "0.0.0",
  "description": "A template for building a React application with Vite, Hono, and Cloudflare Workers",
  "type": "module",
  "scripts": {
    "build": "bun tsgo -b && vite build",
    "cf-typegen": "wrangler types",
    "check": "bun tsgo && vite build && wrangler deploy --dry-run",
    "deploy": "wrangler deploy",
    "dev": "vite",
    "preview": "bun run build && vite preview",
    "test": "vitest --project unit browser",
    "lint": "oxlint --type-aware",
    "format": "oxfmt .",
    "ui:add": "bun run scripts/shadcn-add.ts",
    "ui:organize": "bun run scripts/organize-ui.ts",
    "typecheck": "bun tsgo"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.80.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "hono": "^4.8.1",
    "lucide-react": "^0.563.0",
    "next-themes": "^0.4.6",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.11",
    "vaul": "^1.1.2",
    "wouter": "^3.7.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.22.0",
    "@types/node": "^24.0.0",
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3",
    "@typescript/native-preview": "^7.0.0-dev.20251010.1",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/ui": "^4.0.0",
    "lefthook": "^2.0.0",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.36.0",
    "oxlint-tsgolint": "^0.11.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.9.3",
    "vite": "8.0.0-beta.11",
    "vitest": "^4.0.0",
    "vitest-browser-react": "^2.0.5",
    "wrangler": "^4.61.1"
  },
  "cloudflare": {
    "label": "Hono App Boilerplate",
    "products": [
      "Workers"
    ],
    "categories": [],
    "icon_urls": [
      "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/5ca0ca32-e897-4699-d4c1-6b680512f000/public"
    ],
    "preview_image_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/fc7b4b62-442b-4769-641b-ad4422d74300/public",
    "publish": true
  }
}
